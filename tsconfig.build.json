{
    "compilerOptions": {
        "declaration": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "strict": true,
        "target": "esnext",
        "allowImportingTsExtensions": true,
      },
      "include": ["src/**/*.ts"],
      "exclude": ["src/**/*.test.ts", "src/**/*.spec.ts"],
}